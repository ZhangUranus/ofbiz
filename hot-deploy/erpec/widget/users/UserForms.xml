<forms xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/widget-form.xsd">
    <form name="ViewProfile" type="single">
        <field name="loginId" title="用户账号">
            <display description="${profileVO.loginId}" />
        </field>
        <field name="name" title="用户姓名">
            <display description="${profileVO.name}" />
        </field>
        <field name="telNum" title="联系手机">
            <display description="${profileVO.telNum}" />
        </field>
        <field name="telNum2" title="联系座机">
            <display description="${profileVO.telNum2}" />
        </field>
        <field name="cardId" title="身份证号">
            <display description="${profileVO.cardId}" />
        </field>
        <field name="addr" title="身份证号">
            <display description="${profileVO.addr}" />
        </field>
    </form>
</forms>
